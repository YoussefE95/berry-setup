{
    "workbench.colorTheme": "Everforest Light",
    "editor.fontFamily": "'Hack Nerd Font Mono', monospace",
    "editor.codeLensFontFamily": "'Hack Nerd Font Mono', monospace",
    "debug.console.fontFamily": "'Hack Nerd Font Mono', monospace",
    "terminal.integrated.fontFamily": "'Hack Nerd Font Mono', monospace",
    "window.zoomLevel": 1.25,
    "editor.minimap.enabled": true,
    "editor.formatOnSave": true,
    "explorer.confirmDelete": false,
    "workbench.tree.indent": 24,
    "prettier.tabWidth": 4,
    "prettier.singleQuote": true,
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "prettier.jsxSingleQuote": true,
    "html.completion.attributeDefaultValue": "singlequotes",
    "telemetry.telemetryLevel": "off",
    "editor.detectIndentation": false,
    "everforest.lightContrast": "soft",
    "everforest.darkContrast": "medium",
    "gruvboxMaterial.lightContrast": "soft",
    "gruvboxMaterial.darkContrast": "medium",
    "editor.scrollbar.vertical": "hidden",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "material-product-icons",
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.menuBarVisibility": "toggle",
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false
}
